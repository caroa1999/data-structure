#include <stdio.h>

int main(void) {
	char* ptr[] = { {"Korea"}, {"Seoul"}, {"Mapo"},{"152번지 2/3"} };
	char* ptr1[] = { {"Korea"}, {"Seoul"}, {"Jongno"},{"152번지 2/3"} };


	for (int i = 0; i < 4; i++) {
		printf("%s\n", ptr[i]);
	}

	printf("\n\n");

	for (int i = 0; i < 4; i++) {
		printf("%s\n", ptr1[i]);
	}

	return 0;
}
